  <% if agent_logged_in? %>
    <%= render'layouts/agent_header' %>
  <% end %>


<div class="index-wrapper">
  <p id="notice"><%= notice %></p>
  <h1>Projects</h1>
  <table>
    <thead>
      <tr>
        <th>Project name</th>
        <th>Production co</th>
        <th>Director</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @projects.each do |project| %>
        <tr>
          <td><%= link_to project.project_name, project %></td>
          <td><%= project.production_co %></td>
          <td><%= project.director %></td>
          <td><%= link_to 'Delete', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <div class="proj-link1">
    <%= link_to 'New Project', new_project_path %>
  </div>
</div>

<br>





