<p id="notice"><%= notice %></p>

<h1>Auditions</h1>

<table>
  <thead>
    <tr>
      <th>Attended</th>
      <th>Time</th>
      <th>Actor</th>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auditions.each do |audition| %>
      <tr>
        <td><%= audition.attended %></td>
        <td><%= audition.time %></td>
        <td><%= audition.actor %></td>
        <td><%= audition.role %></td>
        <td><%= link_to 'Show', audition %></td>
        <td><%= link_to 'Edit', edit_audition_path(audition) %></td>
        <td><%= link_to 'Destroy', audition, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Audition', new_audition_path %>
